<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bggrey"
    android:baselineAligned="false"
    android:orientation="vertical"
    >

    <com.google.android.material.card.MaterialCardView
        style="@style/Widget.MaterialComponents.CardView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="16dp"
        android:animateLayoutChanges="true"
        android:elevation="4dp"
        android:translationZ="4dp"
        app:cardBackgroundColor="@color/white"
        app:cardCornerRadius="@dimen/mtrl_card_corner_radius"
        app:cardElevation="4dp"
        tools:ignore="PrivateResource">

        <ScrollView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <com.google.android.material.card.MaterialCardView
                    style="@style/Widget.MaterialComponents.CardView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="20dp"
                    android:layout_marginLeft="@dimen/mtrl_card_spacing"
                    android:layout_marginRight="@dimen/mtrl_card_spacing"
                    android:animateLayoutChanges="true"
                    android:elevation="4dp"
                    android:translationZ="4dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="@dimen/mtrl_card_corner_radius"
                    app:cardElevation="4dp"
                    tools:ignore="PrivateResource">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="8dp"
                        android:animateLayoutChanges="true"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:id="@+id/rlEntry"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/entry"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:text="Entry Chip"
                                android:textAllCaps="true"
                                android:textColor="@color/black"
                                android:textSize="22sp" />
                        </RelativeLayout>

                        <LinearLayout
                            android:id="@+id/llchipEntry"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/rlEntry"
                            android:layout_marginTop="8dp"
                            android:orientation="horizontal"
                            android:visibility="visible"
                            android:weightSum="2"
                            android:baselineAligned="false">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical"
                                android:weightSum="2">

                                <com.google.android.material.chip.Chip
                                    style="@style/Widget.MaterialComponents.Chip.Entry"
                                    android:id="@+id/entryOne"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="8dp"
                                    android:layout_weight="1"
                                    android:checkable="true"
                                    android:text="Airplane"
                                    android:textColor="@color/white"
                                    app:checkedIcon="@drawable/ic_airplane"
                                    app:chipBackgroundColor="@color/colorPrimary"
                                    app:chipIconTint="@color/white" />

                                <com.google.android.material.chip.Chip
                                    style="@style/Widget.MaterialComponents.Chip.Entry"
                                    android:id="@+id/entryTwo"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="8dp"
                                    android:layout_weight="1"
                                    android:checkable="true"
                                    android:text="Biking"
                                    app:checkedIcon="@drawable/ic_direction"
                                    android:textColor="@color/white"
                                    app:chipBackgroundColor="#689F38"
                                    app:chipIconTint="@color/white" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical"
                                android:weightSum="2">

                                <com.google.android.material.chip.Chip
                                    style="@style/Widget.MaterialComponents.Chip.Entry"
                                    android:id="@+id/entryThree"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="8dp"
                                    android:layout_weight="1"
                                    app:checkedIcon="@drawable/ic_location"
                                    android:checkable="true"
                                    android:text="Location"
                                    android:textColor="@color/white"
                                    app:chipBackgroundColor="#689F38"
                                    app:chipIconTint="@color/white" />

                                <com.google.android.material.chip.Chip
                                    style="@style/Widget.MaterialComponents.Chip.Entry"
                                    android:id="@+id/entryFour"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="8dp"
                                    android:layout_weight="1"
                                    android:checkable="true"
                                    app:checkedIcon="@drawable/ic_bus"
                                    android:text="Driving Bus"
                                    android:textColor="@color/white"
                                    app:chipBackgroundColor="@color/colorPrimary"
                                    app:chipIconTint="@color/white" />
                            </LinearLayout>
                        </LinearLayout>
                    </RelativeLayout>
                </com.google.android.material.card.MaterialCardView>

                <com.google.android.material.card.MaterialCardView
                    style="@style/Widget.MaterialComponents.CardView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:layout_marginEnd="20dp"
                    android:animateLayoutChanges="true"
                    android:elevation="4dp"
                    android:translationZ="4dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="@dimen/mtrl_card_corner_radius"
                    app:cardElevation="4dp"
                    tools:ignore="PrivateResource">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="8dp"
                        android:animateLayoutChanges="true"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:id="@+id/rlFilter"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/filter"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:text="Filter Chip"
                                android:textAllCaps="true"
                                android:textColor="@color/black"
                                android:textSize="22sp" />
                        </RelativeLayout>

                        <LinearLayout
                            android:id="@+id/llChipFilter"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/rlFilter"
                            android:layout_marginTop="8dp"
                            android:orientation="horizontal"
                            android:visibility="visible"
                            android:weightSum="2"
                            android:baselineAligned="false">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical"
                                android:weightSum="2">

                                <com.google.android.material.chip.Chip
                                    style="@style/Widget.MaterialComponents.Chip.Filter"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="8dp"
                                    android:layout_weight="1"
                                    android:checkable="true"
                                    android:text="Android"
                                    android:textColor="@color/white"
                                    app:chipBackgroundColor="#a2c74a"
                                    app:chipIconTint="@color/white" />

                                <com.google.android.material.chip.Chip
                                    style="@style/Widget.MaterialComponents.Chip.Filter"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="8dp"
                                    android:layout_weight="1"
                                    android:checkable="true"
                                    android:text="Kotlin"
                                    android:textColor="@color/white"
                                    app:chipBackgroundColor="#e47355"
                                    app:chipIconTint="@color/white" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical"
                                android:weightSum="2">

                                <com.google.android.material.chip.Chip
                                    style="@style/Widget.MaterialComponents.Chip.Filter"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="8dp"
                                    android:layout_weight="1"
                                    android:checkable="true"
                                    android:text="Flutter"
                                    android:textColor="@color/white"
                                    app:chipBackgroundColor="#55c2f7"
                                    app:chipIconTint="@color/white" />

                                <com.google.android.material.chip.Chip
                                    style="@style/Widget.MaterialComponents.Chip.Filter"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="8dp"
                                    android:layout_weight="1"
                                    android:checkable="true"
                                    android:text="Xamarin"
                                    android:textColor="@color/white"
                                    app:chipBackgroundColor="#3895d7"
                                    app:chipIconTint="@color/white" />
                            </LinearLayout>
                        </LinearLayout>
                    </RelativeLayout>
                </com.google.android.material.card.MaterialCardView>

                <com.google.android.material.card.MaterialCardView
                    style="@style/Widget.MaterialComponents.CardView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="20dp"
                    android:animateLayoutChanges="true"
                    android:elevation="4dp"
                    android:translationZ="4dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="@dimen/mtrl_card_corner_radius"
                    app:cardElevation="4dp"
                    tools:ignore="PrivateResource">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="8dp"
                        android:animateLayoutChanges="true"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:id="@+id/rlChoice"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/choice"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:text="Choice Chip"
                                android:textAllCaps="true"
                                android:textColor="@color/black"
                                android:textSize="22sp" />
                        </RelativeLayout>

                        <LinearLayout
                            android:id="@+id/llChipChoice"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/rlChoice"
                            android:layout_marginTop="8dp"
                            android:orientation="horizontal"
                            android:visibility="visible"
                            android:weightSum="2">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical"
                                android:weightSum="2">

                                <com.google.android.material.chip.Chip
                                    style="@style/Widget.MaterialComponents.Chip.Choice"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="8dp"
                                    android:layout_weight="1"
                                    android:checkable="true"
                                    android:text="Marshmallow"
                                    android:textColor="@color/white"
                                    app:chipBackgroundColor="#5C6BC0"
                                    app:chipIconTint="@color/white" />

                                <com.google.android.material.chip.Chip
                                    style="@style/Widget.MaterialComponents.Chip.Choice"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="8dp"
                                    android:layout_weight="1"
                                    android:checkable="true"
                                    android:text="Nougat"
                                    android:textColor="@color/white"
                                    app:chipBackgroundColor="#FFCA28"
                                    app:chipIconTint="@color/white" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical"
                                android:weightSum="2">

                                <com.google.android.material.chip.Chip
                                    style="@style/Widget.MaterialComponents.Chip.Choice"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="8dp"
                                    android:layout_weight="1"
                                    android:checkable="true"
                                    android:text="Oreo"
                                    android:textColor="@color/white"
                                    app:chipBackgroundColor="#8D6E63"
                                    app:chipIconTint="@color/white" />

                                <com.google.android.material.chip.Chip
                                    style="@style/Widget.MaterialComponents.Chip.Choice"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="8dp"
                                    android:layout_weight="1"
                                    android:checkable="true"
                                    android:text="Kitkat"
                                    android:textColor="@color/white"
                                    app:chipBackgroundColor="#EF5350"
                                    app:chipIconTint="@color/white" />
                            </LinearLayout>
                        </LinearLayout>
                    </RelativeLayout>
                </com.google.android.material.card.MaterialCardView>

                <com.google.android.material.card.MaterialCardView
                    style="@style/Widget.MaterialComponents.CardView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:layout_marginEnd="20dp"
                    android:layout_marginBottom="20dp"
                    android:animateLayoutChanges="true"
                    android:elevation="4dp"
                    android:translationZ="4dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="@dimen/mtrl_card_corner_radius"
                    app:cardElevation="4dp"
                    tools:ignore="PrivateResource">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="8dp"
                        android:animateLayoutChanges="true"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:id="@+id/rlAction"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/Choice"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:text="Action Chip"
                                android:textAllCaps="true"
                                android:textColor="@color/black"
                                android:textSize="22sp" />
                        </RelativeLayout>

                        <LinearLayout
                            android:id="@+id/llChipAction"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/rlAction"
                            android:layout_marginTop="8dp"
                            android:orientation="horizontal"
                            android:visibility="visible"
                            android:weightSum="2">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical"
                                android:weightSum="2">

                                <com.google.android.material.chip.Chip
                                    style="@style/Widget.MaterialComponents.Chip.Action"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="8dp"
                                    android:layout_weight="1"
                                    android:checkable="true"
                                    android:text="Sagar Patel"
                                    android:textColor="@color/white"
                                    app:checkedIcon="@drawable/ic_account"
                                    app:chipBackgroundColor="#6300e8"
                                    app:chipIconTint="@color/white" />

                                <com.google.android.material.chip.Chip
                                    style="@style/Widget.MaterialComponents.Chip.Action"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="8dp"
                                    android:layout_weight="1"
                                    android:checkable="true"
                                    android:text="Shashi Patel"
                                    app:checkedIcon="@drawable/ic_account"
                                    android:textColor="@color/white"
                                    app:chipBackgroundColor="#4daf58"
                                    app:chipIconTint="@color/white" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical"
                                android:weightSum="2">

                                <com.google.android.material.chip.Chip
                                    style="@style/Widget.MaterialComponents.Chip.Action"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="8dp"
                                    android:layout_weight="1"
                                    android:checkable="true"
                                    android:text="Divyesh"
                                    app:checkedIcon="@drawable/ic_account"
                                    android:textColor="@color/white"
                                    app:chipBackgroundColor="#689F38"
                                    app:chipIconTint="@color/white" />

                                <com.google.android.material.chip.Chip
                                    style="@style/Widget.MaterialComponents.Chip.Action"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="8dp"
                                    android:layout_weight="1"
                                    android:checkable="true"
                                    android:text="Milan"
                                    app:checkedIcon="@drawable/ic_account"
                                    android:textColor="@color/white"
                                    app:chipBackgroundColor="@color/colorPrimary"
                                    app:chipIconTint="@color/white" />
                            </LinearLayout>
                        </LinearLayout>
                    </RelativeLayout>
                </com.google.android.material.card.MaterialCardView>
            </LinearLayout>
        </ScrollView>
    </com.google.android.material.card.MaterialCardView>
</LinearLayout>
